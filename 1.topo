[settings]
capacity = static(25Mbps)
latency = static(10ms)
seed = 7

[topology]
host_1 -> d_a weight=1
host_2 -> d_a weight=2
host_3 -> d_b weight=2
d_a -> d_b weight=1 capacity=8Mbps
d_b -> host_dest capacity=10Mbps

[aggregates]
host_1 -> host_dest cubic=1 [d_a, d_b]
host_2 -> host_dest cubic=1 [d_a, d_b]
host_3 -> host_dest cubic=1 [d_b]